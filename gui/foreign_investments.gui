types foreign_investments_types
{
	type fi_right_click_menu_item = dynamicgridbox {
		visible = "[Not(StatesPanel.AccessState.GetCountry.IsLocalPlayer)]"
		datamodel = "[StatesPanel.AccessState.AccessPotentialUrbanBuildings]"
		item = {
			button = {
				size = { 360 44 }
				using = default_button
				align = left|nobaseline
				using = fontsize_large
				icon = {
					size = { 44 44 }
				}
				block "properties" {}
				tooltip = "FI_EXPAND_TOOLTIP2"
				onclick = "[ScriptedGui.Execute(GuiScope.AddScope('invested_state', StatesPanel.AccessState.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
				enabled = "[ScriptedGui.IsValid(GuiScope.AddScope('invested_state', StatesPanel.AccessState.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
				hbox = {
					layoutpolicy_horizontal = expanding
					layoutpolicy_vertical = expanding
					margin_left  = 10
					icon = {
						texture = "[Building.GetTexture]"
						size = { 28 28 }
					}
					textbox = {
						layoutpolicy_horizontal = expanding
						layoutpolicy_vertical = expanding
						text = "[Building.GetNameNoFormatting]"
						align = left|nobaseline
						fontsize_min = 12
						using = fontsize_large
						elide = right
						margin = { 5 0 }
					}
				}
				margin_left = 54		
				onclick = "[RightClickMenuManager.Close]"
				using = build_button_sound
			}
		}
	}
	type fi_button_icon_minus_action = button_icon_minus_action {
		size = { 29 29 }
		parentanchor = bottom
		position = { 5 0 }
		block "building_type" {}
		tooltip = "FI_DOWNSIZE_TOOLTIP"
		enabled = "[ScriptedGui.IsValid(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('invested_state', State.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
		onclick = "[ScriptedGui.Execute(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('invested_state', State.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
		onclick = "[Execute(GetPlayer.GetCapital.GetMarket.GetMarketCapital.GetBuilding('building_investment_industry').Downsize)]"
		using = tooltip_below
	}
	type fi_button_icon_plus_action = button_icon_plus_action {
		position = { 102 0 }
		size = { 29 29 }
		block "building_type" {}
		tooltip = "FI_EXPAND_TOOLTIP"
		onclick = "[ScriptedGui.Execute(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('invested_state', State.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
		enabled = "[ScriptedGui.IsValid(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('invested_state', State.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
		onrightclick = "[ScriptedGui.Execute(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('invested_state', State.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).AddScope('is_right_click', GetPlayer.MakeScope).End)]"
	}
	type fi_bar_textbox = button {
		block "building_type" {}
		size = { 53 20 }
		align = center|nobaseline
		fontsize = 17
		parentanchor = vcenter
		position = { 4 0 }
		datacontext = "[GetScriptedGui('buy_share_scripted_gui')]"
		enabled = "[ScriptedGui.IsValid(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
		onclick = "[ScriptedGui.Execute(GuiScope.AddScope('invested_building', Building.MakeScope).AddScope('investor_country', GetPlayer.MakeScope).End)]"
